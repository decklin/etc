keystate_numlock = disable
keystate_capslock = disable
keystate_scrolllock = disable

"setxkbmap us; xmodmap ~/.Xmodmap"
    Shift + Shift_L
"setxkbmap dvorak; xmodmap ~/.Xmodmap"
    Shift + Shift_R

"aedesk 0"
    Alt + F1
"aedesk 1"
    Alt + F2
"aedesk 2"
    Alt + F3
"aedesk 3"
    Alt + F4
"aedesk -1"
    XF86Back
"aedesk +1"
    XF86Forward

"mop --prev --show"
    XF86AudioPrev
"mop --next --show"
    XF86AudioNext
"mop --stop --show"
    XF86AudioStop
"mop --toggle --show"
    XF86AudioPlay
"mop --vol -5 --showvol"
    XF86AudioLowerVolume
"mop --vol +5 --showvol"
    XF86AudioRaiseVolume
"mop --vol 0"
    XF86AudioMute

"osdtime"
    Print
"xscreensaver-command -lock"
    Pause
"xsel | xargs -0 xdotool type"
    Insert + Release
"dbrowse"
    Control+Alt + backslash
"dbrowse --run"
    Control+Alt + space
"xopenurls"
    b:8
"xdotool key space"
    b:9

"xdotool click 4"
    Control+Alt + Prior
"xdotool click 5"
    Control+Alt + Next
