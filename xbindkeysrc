keystate_numlock = disable
keystate_capslock = disable
keystate_scrolllock = disable

"setxkbmap us; xmodmap ~/.Xmodmap"
    Shift + Shift_L
"setxkbmap dvorak; xmodmap ~/.Xmodmap"
    Shift + Shift_R

"aedesk -1"
    XF86Back
"aedesk +1"
    XF86Forward
"aedesk 0"
    Alt + F1
"aedesk 1"
    Alt + F2
"aedesk 2"
    Alt + F3
"aedesk 3"
    Alt + F4
"xsetdesk 0"
    Mod4 + F1
"xsetdesk 1"
    Mod4 + F2
"xsetdesk 2"
    Mod4 + F3
"xsetdesk 3"
    Mod4 + F4

"mop --prev --show"
    XF86AudioPrev
"mop --next --show"
    XF86AudioNext
"mop --stop --show"
    XF86AudioStop
"mop --toggle --show"
    XF86AudioPlay
"mop --vol -5 --showvol"
    XF86AudioLowerVolume
"mop --vol +5 --showvol"
    XF86AudioRaiseVolume
"mop --vol 0 --showvol"
    XF86AudioMute

"xsel | xargs -0 xdotool type --delay 0"
    Insert + Release
"dbrowse --run"
    Mod4 + Tab
"xsel | xargs -0 open-url"
    b:8
"xdotool key space"
    b:9

"xdotool click 4"
    Mod4 + Prior
"xdotool click 5"
    Mod4 + Next

"sudo pm-suspend"
    XF86Sleep
"sudo flipbright"
    XF86Battery
"aelock --blank"
    XF86ScreenSaver
