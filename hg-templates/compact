changeset_quiet = '{node|short} :: {date|isodate} {author|user} :: {rev}{parents}{branches}\n - {desc|fill68|firstline}\n'
start_branches = ' ('
branch = '{branch},'
last_branch = '{branch})'
start_tags = ' ('
tag = '{tag},'
last_tag = '{tag})'
start_parents = ':'
parent = '{rev},'
last_parent = '{rev}'
