gtk-key-theme-name = "Emacs"
gtk-theme-name = "Mist"
gtk-font-name = "Sans 8"
gtk-toolbar-style = GTK_TOOLBAR_BOTH

binding "gtk-emacs-text-entry-extra" {
  bind "<alt>BackSpace" { "delete-from-cursor" (word-ends, -1) }
}

class "GtkTextView" binding "gtk-emacs-text-entry-extra"
class "GtkEntry" binding "gtk-emacs-text-entry-extra"

include ".gtkrc-2.0-local"
